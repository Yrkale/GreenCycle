spring.application.name=GreenCycle

# DB Config
spring.datasource.url=jdbc:mysql://localhost:3306/greencycle_db?useSSL=false
spring.datasource.username=root
spring.datasource.password=root

# JPA Config
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

# JWT Config
greencycle.app.jwtSecret=J1SathiSecretKeyForJWTToken
greencycle.app.jwtExpirationMs=86400000

# CORS and Port
server.port=8080